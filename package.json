{
  "name": "med-scan-viewer",
  "version": "1.0.0",
  "description": "A medical image viewer for DICOM files",
  "main": "main.js",
  "type": "commonjs",
  "scripts": {
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "electron-builder",
    "build:win": "electron-builder --win"
  },
  "keywords": [],
  "author": {
    "name": "OrbitexLabs",
    "email": "orbitexlabs@gmail.com"
  },
  "homepage": "https://orbitexlabs.com.br",
  "license": "ISC",
  "devDependencies": {
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8",
    "electron-rebuild": "^3.2.9"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.15",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.15",
    "@nestjs/platform-express": "^10.4.15",
    "@radicalimaging/static-cs-lite": "^1.5.0",
    "@radicalimaging/static-wado-plugins": "^1.5.0",
    "chokidar": "^3.6.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dcmjs": "^0.35.0",
    "dicom-parser": "^1.8.21",
    "dot-env": "^0.0.1",
    "electron-is-dev": "^3.0.1",
    "express": "^4.21.2",
    "formidable": "^3.5.2",
    "i18next": "^24.1.0",
    "i18next-fs-backend": "^2.6.0",
    "i18next-http-middleware": "^3.7.0",
    "ini": "^5.0.0",
    "morgan": "^1.10.0",
    "nestjs-i18n": "^10.5.0",
    "node-notifier": "^10.0.1",
    "node-object-hash": "^3.0.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "typescript": "5.6.3"
  },
  "build": {
    "appId": "com.orbitexlabs.medscanviewer",
    "productName": "MEDScan Viewer",
    "files": [
      "node_modules/**/*",
      "apps/**/*",
      "styles/**/*",
      "main.js",
      "logo.ico",
      "config.html",
      "config.js"
    ],
    "extraResources": [
      {
        "from": "./node_modules/node/",
        "to": "node"
      }
    ],
    "directories": {
      "output": "release-builds"
    },
    "win": {
      "target": "nsis",
      "icon": "logo.ico"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Health",
      "maintainer": "OrbitexLabs orbitexlabs@gmail.com",
      "icon": "logo.png"
    }
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
